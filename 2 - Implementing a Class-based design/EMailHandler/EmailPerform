import javax.swing.JOptionPane;

public class EmailPerform {
  public static void emailPerform(String email){
    
    
    //Success-Messagebox appears
    String finishEmail =  email + " has \nreceived the message";
    
    JOptionPane.showMessageDialog(Main.frame, finishEmail);
    
    
  }
}
